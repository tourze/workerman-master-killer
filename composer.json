{
  "name": "tourze/workerman-master-killer",
  "description": "Kill Workerman master process, inspired by https://www.workerman.net/q/7958",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-posix": "*",
    "ext-pcntl": "*",
    "workerman/workerman": "^5.1",
    "psr/log": "^3|^2|^1"
  },
  "autoload": {
    "psr-4": {
